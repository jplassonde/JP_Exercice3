                   ENDIF ; SYSTEM_LARGE_MEMORY_MODEL
                   ENDIF ; RAM_USE_CLASS_2
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_3 )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                   IF ( SYSTEM_IDXPG_TRACKS_STK_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and   F, ~FLAG_PGMODE_MASK        ; NOTE: transition thru 00b state
                      or    F,  FLAG_PGMODE_MASK & FLAG_PGMODE_11b
                   ENDIF
                   ENDIF ;  PGMODE LOCKED
                   IF ( SYSTEM_IDXPG_TRACKS_IDX_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and   F, ~FLAG_PGMODE_MASK        ; NOTE: transition thru 00b state
                      or    F,  FLAG_PGMODE_MASK & FLAG_PGMODE_10b
                   ENDIF
                   ENDIF ; PGMODE FREE
                   ENDIF ; SYSTEM_LARGE_MEMORY_MODEL
                   ENDIF ; RAM_USE_CLASS_3
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_4 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_4
                
                    ret
                .ENDSECTION
                
                .SECTION
                ;-----------------------------------------------------------------------------
                ;  FUNCTION NAME: OneWire_DS_fFindNext
                ;
                ;  DESCRIPTION:
                ;     Find the first device on the 1-Wire bus.
                ;-----------------------------------------------------------------------------
                ;
                ;  ARGUMENTS:
                ;     none.
                ;
                ;  RAM BUFFER CONTENTS:
                ;     OneWire_DS_ROM = ROM number of 1-Wire device
                ;
                ;  RETURNS:
                ;     A = 01h if device found
                ;     A = 00h if device not found, end of search
                ;
                ;  SIDE EFFECTS:
                ;    The A and X registers may be modified by this or future implementations
                ;    of this function.  The same is true for all RAM page pointer registers in
                ;    the Large Memory Model.  When necessary, it is the calling function's
                ;    responsibility to preserve their values across calls to fastcall16
                ;    functions.  Currently only the page pointer registers listed below are modified:
                ;    CUR_PP
                ;    IDX_PP
                ;-----------------------------------------------------------------------------
                 OneWire_DS_fFindNext:
                _OneWire_DS_fFindNext:
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_1 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_1
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_2 )
                      IF ( SYSTEM_IDXPG_TRACKS_IDX_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      or   F, FLAG_PGMODE_01b
                   ENDIF ;  SYSTEM_LARGE_MEMORY_MODEL
                      ENDIF
                   ENDIF ; RAM_USE_CLASS_2
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_3 )
                      IF ( SYSTEM_IDXPG_TRACKS_STK_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and  F, ~FLAG_PGMODE_01b
                   ENDIF ;  SYSTEM_LARGE_MEMORY_MODEL
                      ENDIF
                   ENDIF ; RAM_USE_CLASS_3
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_4 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_4
                
                
                    call  OneWire_DS_Search
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_1 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_1
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_2 )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                   IF ( SYSTEM_IDXPG_TRACKS_STK_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and   F, ~FLAG_PGMODE_MASK        ; NOTE: transition thru 00b state
                      or    F,  FLAG_PGMODE_MASK & FLAG_PGMODE_11b
                   ENDIF
                   ENDIF ;  PGMODE LOCKED
                   IF ( SYSTEM_IDXPG_TRACKS_IDX_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and   F, ~FLAG_PGMODE_MASK        ; NOTE: transition thru 00b state
                      or    F,  FLAG_PGMODE_MASK & FLAG_PGMODE_10b
                   ENDIF
                   ENDIF ; PGMODE FREE
                   ENDIF ; SYSTEM_LARGE_MEMORY_MODEL
                   ENDIF ; RAM_USE_CLASS_2
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_3 )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                   IF ( SYSTEM_IDXPG_TRACKS_STK_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and   F, ~FLAG_PGMODE_MASK        ; NOTE: transition thru 00b state
                      or    F,  FLAG_PGMODE_MASK & FLAG_PGMODE_11b
                   ENDIF
                   ENDIF ;  PGMODE LOCKED
                   IF ( SYSTEM_IDXPG_TRACKS_IDX_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and   F, ~FLAG_PGMODE_MASK        ; NOTE: transition thru 00b state
                      or    F,  FLAG_PGMODE_MASK & FLAG_PGMODE_10b
                   ENDIF
                   ENDIF ; PGMODE FREE
                   ENDIF ; SYSTEM_LARGE_MEMORY_MODEL
                   ENDIF ; RAM_USE_CLASS_3
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_4 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_4
                
                    ret
                .ENDSECTION
                
                .SECTION
                ;-----------------------------------------------------------------------------
                ;  FUNCTION NAME: OneWire_DS_Search
                ;
                ;  DESCRIPTION:
                ;     Perform the 1-Wire search algorithm with the current search state.
                ;-----------------------------------------------------------------------------
                ;  ARGUMENTS:
                ;     none.
                ;
                ;  RAM BUFFER CONTENTS:
                ;     OneWire_DS_ROM = ROM number of 1-Wire device
                ;
                ;  RETURNS:
                ;     A = 01h if device found
                ;     A = 00h if device not found, end of search
                ;
                ;  SIDE EFFECTS:
                ;    The A and X registers may be modified by this or future implementations
                ;    of this function.  The same is true for all RAM page pointer registers in
                ;    the Large Memory Model.  When necessary, it is the calling function's
                ;    responsibility to perserve their values across calls to fastcall16
                ;    functions. Currently only the page pointer registers listed below are modified:
                ;    CUR_PP
                ;    IDX_PP
                ;-----------------------------------------------------------------------------
                ID_BIT_NUMBER:              equ   0
                LAST_ZERO:                  equ   1
                ROM_BYTE_NUMBER:            equ   2
                SEARCH_RESULT:              equ   3
                ID_BIT:                     equ   4
                CMP_ID_BIT:                 equ   5
                ROM_BYTE_MASK:              equ   6
                SEARCH_DIRECTION:           equ   7
                
                 OneWire_DS_Search:
                _OneWire_DS_Search:
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_1 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_1
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_2 )
                      IF ( SYSTEM_IDXPG_TRACKS_IDX_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      or   F, FLAG_PGMODE_01b
                   ENDIF ;  SYSTEM_LARGE_MEMORY_MODEL
                      ENDIF
                   ENDIF ; RAM_USE_CLASS_2
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_3 )
                      IF ( SYSTEM_IDXPG_TRACKS_STK_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and  F, ~FLAG_PGMODE_01b
                   ENDIF ;  SYSTEM_LARGE_MEMORY_MODEL
                      ENDIF
                   ENDIF ; RAM_USE_CLASS_3
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_4 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_4
                
                   IF ( RAM_USE_CLASS_3 & RAM_USE_CLASS_1 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_1
                
                   IF ( RAM_USE_CLASS_3 & RAM_USE_CLASS_2 )
                      IF ( SYSTEM_IDXPG_TRACKS_IDX_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      or   F, FLAG_PGMODE_01b
                   ENDIF ;  SYSTEM_LARGE_MEMORY_MODEL
                      ENDIF
                   ENDIF ; RAM_USE_CLASS_2
                
                   IF ( RAM_USE_CLASS_3 & RAM_USE_CLASS_3 )
                      IF ( SYSTEM_IDXPG_TRACKS_STK_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and  F, ~FLAG_PGMODE_01b
                   ENDIF ;  SYSTEM_LARGE_MEMORY_MODEL
                      ENDIF
                   ENDIF ; RAM_USE_CLASS_3
                
                   IF ( RAM_USE_CLASS_3 & RAM_USE_CLASS_4 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_4
                
                
                    mov   X,SP                                   ; Get location of stack
                    add   SP,08h                                 ; Make locations for local variables
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      IF ( SYSTEM_MULTIPAGE_STACK )
                         mov   A, reg[STK_PP]
                         mov   reg[IDX_PP], A
                      ELSE
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[IDX_PP], SYSTEM_STACK_PAGE
                   ENDIF
                      ENDIF
                   ENDIF
                    mov   [X+ID_BIT_NUMBER],01h                  ; Initialize for search
                    mov   [X+LAST_ZERO],00h
                    mov   [X+ROM_BYTE_NUMBER],00h
                    mov   [X+ROM_BYTE_MASK],01h
                    mov   [X+SEARCH_RESULT],00h
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[CUR_PP], >OneWire_DS_bCRC8Value
                   ENDIF
                    mov   [OneWire_DS_bCRC8Value],00h
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[CUR_PP], >OneWire_DS_LastDeviceFlag
                   ENDIF
                    mov   A,[OneWire_DS_LastDeviceFlag]
                    jnz   .check_result
                    call  OneWire_DS_fReset
                    jz    .search_return_false
                
                    mov   A,F0h                                  ; Search command
                    push  X
                    call  OneWire_DS_WriteByte
                    pop   X
                .search_loop:
                    call  OneWire_DS_bReadBit
                    mov   [X+ID_BIT],A
                    call  OneWire_DS_bReadBit
                    mov   [X+CMP_ID_BIT],A
                    and   A,01h
                    and   A,[X+ID_BIT]
                    jnz   .check_search
                
                .search_else_1:
                    mov   A,[X+ID_BIT]                           ; If ID_BIT != CMP_ID_BIT
                    cmp   A,[X+CMP_ID_BIT]
                    jz    .search_else_2
                    mov   A,[X+ID_BIT]
                    mov   [X+SEARCH_DIRECTION],A
                    jmp   .search_set_or_clear
                .search_else_2:
                    mov   A,[X+ID_BIT_NUMBER]
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[CUR_PP], >OneWire_DS_LastDiscrepancy
                   ENDIF
                    cmp   A,[OneWire_DS_LastDiscrepancy]
                    jnc   .search_else_3
                    push  X
                    mov   X,[X+ROM_BYTE_NUMBER]
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[IDX_PP], >OneWire_DS_ROM
                   ENDIF
                    mov   A,[X+OneWire_DS_ROM]
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      IF ( SYSTEM_MULTIPAGE_STACK )
                         mov   A, reg[STK_PP]
                         mov   reg[IDX_PP], A
                      ELSE
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[IDX_PP], SYSTEM_STACK_PAGE
                   ENDIF
                      ENDIF
                   ENDIF
                    pop   X
                    and   A,[X+ROM_BYTE_MASK]
                    jnz   .search_dir_1
                    mov   [X+SEARCH_DIRECTION],00h
                    jmp   .search_is_dir_0
                .search_dir_1:
                    mov   [X+SEARCH_DIRECTION],01h
                    jmp   .search_is_dir_0
                .search_else_3:
                    mov   A,[X+ID_BIT_NUMBER]
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[CUR_PP], >OneWire_DS_LastDiscrepancy
                   ENDIF
                    cmp   A,[OneWire_DS_LastDiscrepancy]         ;SEARCH_DIRECTION = (ID_BIT_NUMBER == LastDiscrepancy search_direction)
                    jnz   .search_dir_1_2
                    mov   [X+SEARCH_DIRECTION],01h
                    jmp   .search_is_dir_0
                .search_dir_1_2:
                    mov   [X+SEARCH_DIRECTION],00h
                .search_is_dir_0:
                    mov   A,[X+SEARCH_DIRECTION]
                    jnz   .search_set_or_clear
                    mov   A,[X+ID_BIT_NUMBER]
                    mov   [X+LAST_ZERO],A
                    cmp   A,09h
                    jnc   .search_set_or_clear
                    mov   A,[X+LAST_ZERO]
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[CUR_PP], >OneWire_DS_LastFamilyDiscrepancy
                   ENDIF
                    mov   [OneWire_DS_LastFamilyDiscrepancy],A
                .search_set_or_clear:
                    cmp   [X+SEARCH_DIRECTION],01h
                    jnz   .search_else_4
                    mov   A,[X+ROM_BYTE_MASK]
                    push  X
                    mov   X,[X+ROM_BYTE_NUMBER]
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[IDX_PP], >OneWire_DS_ROM
                   ENDIF
                    or    [X+OneWire_DS_ROM],A
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      IF ( SYSTEM_MULTIPAGE_STACK )
                         mov   A, reg[STK_PP]
                         mov   reg[IDX_PP], A
                      ELSE
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[IDX_PP], SYSTEM_STACK_PAGE
                   ENDIF
                      ENDIF
                   ENDIF
                    pop   X
                    jmp   .search_write_direction
                .search_else_4:
                    mov   A,[X+ROM_BYTE_MASK]
                    push  X
                    mov   X,[X+ROM_BYTE_NUMBER]
                    cpl   A
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[IDX_PP], >OneWire_DS_ROM
                   ENDIF
                    and   [X+OneWire_DS_ROM],A
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      IF ( SYSTEM_MULTIPAGE_STACK )
                         mov   A, reg[STK_PP]
                         mov   reg[IDX_PP], A
                      ELSE
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[IDX_PP], SYSTEM_STACK_PAGE
                   ENDIF
                      ENDIF
                   ENDIF
                    pop   X
                .search_write_direction:
                    mov   A,[X+SEARCH_DIRECTION]
                    call  OneWire_DS_WriteBit
                    inc   [X+ID_BIT_NUMBER]
                    asl   [X+ROM_BYTE_MASK]
                    cmp   [X+ROM_BYTE_MASK],00h
                    jnz   .check_if_loop
                    push  X
                    mov   X,[X+ROM_BYTE_NUMBER]
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[IDX_PP], >OneWire_DS_ROM
                   ENDIF
                    mov   A,[X+OneWire_DS_ROM]
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      IF ( SYSTEM_MULTIPAGE_STACK )
                         mov   A, reg[STK_PP]
                         mov   reg[IDX_PP], A
                      ELSE
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[IDX_PP], SYSTEM_STACK_PAGE
                   ENDIF
                      ENDIF
                   ENDIF
                    call  OneWire_DS_bCRC8
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_1 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_1
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_2 )
                      IF ( SYSTEM_IDXPG_TRACKS_IDX_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      or   F, FLAG_PGMODE_01b
                   ENDIF ;  SYSTEM_LARGE_MEMORY_MODEL
                      ENDIF
                   ENDIF ; RAM_USE_CLASS_2
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_3 )
                      IF ( SYSTEM_IDXPG_TRACKS_STK_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and  F, ~FLAG_PGMODE_01b
                   ENDIF ;  SYSTEM_LARGE_MEMORY_MODEL
                      ENDIF
                   ENDIF ; RAM_USE_CLASS_3
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_4 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_4
                
                    pop   X
                    inc   [X+ROM_BYTE_NUMBER]
                    mov   [X+ROM_BYTE_MASK],01h
                .check_if_loop:
                    cmp   [X+ROM_BYTE_NUMBER],08h
                    jc    .search_loop
                .check_search:
                    cmp   [X+ID_BIT_NUMBER],41h
                    jc    .check_result
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[CUR_PP], >OneWire_DS_bCRC8Value
                   ENDIF
                    cmp   [OneWire_DS_bCRC8Value],00h
                    jnz   .check_result
                    mov   A,[X+LAST_ZERO]
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[CUR_PP], >OneWire_DS_LastDiscrepancy
                   ENDIF
                    mov   [OneWire_DS_LastDiscrepancy],A
                    jnz   .search_not_last
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[CUR_PP], >OneWire_DS_LastDeviceFlag
                   ENDIF
                    mov   [OneWire_DS_LastDeviceFlag],01h
                .search_not_last:
                    mov   [X+SEARCH_RESULT],01h
                ; If no device found then reset counter so next search will be like a first
                .check_result:
                    mov   A,[X+SEARCH_RESULT]
                    jz    .search_return_false
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[CUR_PP], >OneWire_DS_ROM
                   ENDIF
                    mov   A,[OneWire_DS_ROM+0]
                    jz    .search_return_false
                .search_return_true:
                    mov   A,01h
                    jmp   .search_clean_up
                .search_return_false:
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[CUR_PP], >OneWire_DS_LastDiscrepancy
                   ENDIF
                    mov   [OneWire_DS_LastDiscrepancy],00h
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[CUR_PP], >OneWire_DS_LastDeviceFlag
                   ENDIF
                    mov   [OneWire_DS_LastDeviceFlag],00h
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      mov reg[CUR_PP], >OneWire_DS_LastFamilyDiscrepancy
                   ENDIF
                    mov   [OneWire_DS_LastFamilyDiscrepancy],00h
                    mov   A,00h
                .search_clean_up:
                    add   SP,-08h                                ; Restore SP
                
                   IF ( RAM_USE_CLASS_3 & RAM_USE_CLASS_1 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_1
                
                   IF ( RAM_USE_CLASS_3 & RAM_USE_CLASS_2 )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                   IF ( SYSTEM_IDXPG_TRACKS_STK_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and   F, ~FLAG_PGMODE_MASK        ; NOTE: transition thru 00b state
                      or    F,  FLAG_PGMODE_MASK & FLAG_PGMODE_11b
                   ENDIF
                   ENDIF ;  PGMODE LOCKED
                   IF ( SYSTEM_IDXPG_TRACKS_IDX_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and   F, ~FLAG_PGMODE_MASK        ; NOTE: transition thru 00b state
                      or    F,  FLAG_PGMODE_MASK & FLAG_PGMODE_10b
                   ENDIF
                   ENDIF ; PGMODE FREE
                   ENDIF ; SYSTEM_LARGE_MEMORY_MODEL
                   ENDIF ; RAM_USE_CLASS_2
                
                   IF ( RAM_USE_CLASS_3 & RAM_USE_CLASS_3 )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                   IF ( SYSTEM_IDXPG_TRACKS_STK_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and   F, ~FLAG_PGMODE_MASK        ; NOTE: transition thru 00b state
                      or    F,  FLAG_PGMODE_MASK & FLAG_PGMODE_11b
                   ENDIF
                   ENDIF ;  PGMODE LOCKED
                   IF ( SYSTEM_IDXPG_TRACKS_IDX_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and   F, ~FLAG_PGMODE_MASK        ; NOTE: transition thru 00b state
                      or    F,  FLAG_PGMODE_MASK & FLAG_PGMODE_10b
                   ENDIF
                   ENDIF ; PGMODE FREE
                   ENDIF ; SYSTEM_LARGE_MEMORY_MODEL
                   ENDIF ; RAM_USE_CLASS_3
                
                   IF ( RAM_USE_CLASS_3 & RAM_USE_CLASS_4 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_4
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_1 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_1
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_2 )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                   IF ( SYSTEM_IDXPG_TRACKS_STK_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and   F, ~FLAG_PGMODE_MASK        ; NOTE: transition thru 00b state
                      or    F,  FLAG_PGMODE_MASK & FLAG_PGMODE_11b
                   ENDIF
                   ENDIF ;  PGMODE LOCKED
                   IF ( SYSTEM_IDXPG_TRACKS_IDX_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and   F, ~FLAG_PGMODE_MASK        ; NOTE: transition thru 00b state
                      or    F,  FLAG_PGMODE_MASK & FLAG_PGMODE_10b
                   ENDIF
                   ENDIF ; PGMODE FREE
                   ENDIF ; SYSTEM_LARGE_MEMORY_MODEL
                   ENDIF ; RAM_USE_CLASS_2
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_3 )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                   IF ( SYSTEM_IDXPG_TRACKS_STK_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and   F, ~FLAG_PGMODE_MASK        ; NOTE: transition thru 00b state
                      or    F,  FLAG_PGMODE_MASK & FLAG_PGMODE_11b
                   ENDIF
                   ENDIF ;  PGMODE LOCKED
                   IF ( SYSTEM_IDXPG_TRACKS_IDX_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and   F, ~FLAG_PGMODE_MASK        ; NOTE: transition thru 00b state
                      or    F,  FLAG_PGMODE_MASK & FLAG_PGMODE_10b
                   ENDIF
                   ENDIF ; PGMODE FREE
                   ENDIF ; SYSTEM_LARGE_MEMORY_MODEL
                   ENDIF ; RAM_USE_CLASS_3
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_4 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_4
                
                    ret
                .ENDSECTION
                
                .SECTION
                ;-----------------------------------------------------------------------------
                ;  FUNCTION NAME: OneWire_DS_fVerify
                ;
                ;  DESCRIPTION:
                ;     Verify the device with the ROM number in OneWire_DS_ROM is present.
                ;-----------------------------------------------------------------------------
                ;  ARGUMENTS:
                ;     none.
                ;
                ;  RAM BUFFER CONTENTS:
                ;     OneWire_DS_ROM = ROM number of 1-Wire device
                ;
                ;  RETURNS:
                ;     A = 01h if device found
                ;     A = 00h if device not found
                ;
                ;  SIDE EFFECTS:
                ;    The A and X registers may be modified by this or future implementations
                ;    of this function.  The same is true for all RAM page pointer registers in
                ;    the Large Memory Model.  When necessary, it is the calling function's
                ;    responsibility to perserve their values across calls to fastcall16
                ;    functions. Currently only the page pointer registers listed below are modified:
                ;    CUR_PP, IDX_PP
                ;-----------------------------------------------------------------------------
                ROM_BACKUP:                 equ   0
                LD_BACKUP:                  equ   8
                LDF_BACKUP:                 equ   9
                LFD_BACKUP:                 equ   0x0A
                
                 OneWire_DS_fVerify:
                _OneWire_DS_fVerify:
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_1 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_1
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_2 )
                      IF ( SYSTEM_IDXPG_TRACKS_IDX_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      or   F, FLAG_PGMODE_01b
                   ENDIF ;  SYSTEM_LARGE_MEMORY_MODEL
                      ENDIF
                   ENDIF ; RAM_USE_CLASS_2
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_3 )
                      IF ( SYSTEM_IDXPG_TRACKS_STK_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and  F, ~FLAG_PGMODE_01b
                   ENDIF ;  SYSTEM_LARGE_MEMORY_MODEL
                      ENDIF
                   ENDIF ; RAM_USE_CLASS_3
                
                   IF ( RAM_USE_CLASS_4 & RAM_USE_CLASS_4 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_4
                
                   IF ( RAM_USE_CLASS_3 & RAM_USE_CLASS_1 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_1
                
                   IF ( RAM_USE_CLASS_3 & RAM_USE_CLASS_2 )
                      IF ( SYSTEM_IDXPG_TRACKS_IDX_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      or   F, FLAG_PGMODE_01b
                   ENDIF ;  SYSTEM_LARGE_MEMORY_MODEL
                      ENDIF
                   ENDIF ; RAM_USE_CLASS_2
                
                   IF ( RAM_USE_CLASS_3 & RAM_USE_CLASS_3 )
                      IF ( SYSTEM_IDXPG_TRACKS_STK_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and  F, ~FLAG_PGMODE_01b
                   ENDIF ;  SYSTEM_LARGE_MEMORY_MODEL
                      ENDIF
                   ENDIF ; RAM_USE_CLASS_3
                
                   IF ( RAM_USE_CLASS_3 & RAM_USE_CLASS_4 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_4
                
                
                    mov   X, SP                                  ; Get location of stack
                    add   SP, 0Bh                                ; Make locations for local variables
                IF ( SYSTEM_LARGE_MEMORY_MODEL )
                    mov   A, reg[STK_PP]                         ; Set A to right page to back up ROM to stack
                ELSE
                    mov   A, 00h
